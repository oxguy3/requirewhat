{
    "name": "Google",
    "domains": [
        "accounts.google.com",
        "myaccount.google.com"
    ],
    "password_rules": [
        {
            "@type": "length",
            "@rule": "allow",
            "min": 8,
            "max": 100
        },
        {
            "@type": "char",
            "@rule": "allow",
            "classes": [
                "print"
            ]
        },
        {
            "@type": "char",
            "@rule": "deny",
            "classes": [
                "space"
            ],
            "match": [
                "start",
                "end"
            ]
        },
        {
            "@type": "personal",
            "@rule": "deny",
            "items": [
                "previous_password"
            ]
        }
    ]
}
