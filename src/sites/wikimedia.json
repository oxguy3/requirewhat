{
    "name": "Wikimedia",
    "domains": [
        "*.wikipedia.org",
        "*.wiktionary.org",
        "*.wikiquote.org",
        "*.wikibooks.org",
        "wikisource.org",
        "*.wikisource.org",
        "*.wikinews.org",
        "*.wikiversity.org",
        "www.mediawiki.org",
        "*.wikidata.org",
        "*.wikivoyage.org",
        "*.wikimedia.org"
    ],
    "password_rules": [
        {
            "@type": "length",
            "@rule": "require",
            "min": 1
        },
        {
            "@type": "string",
            "@rule": "deny",
            "strings": [
                "mediawiki",
                "wiki",
                "wikipedia",
                "wiktionary",
                "wikiquote",
                "wikibooks",
                "wikisource",
                "wikinews",
                "wikiversity",
                "mediawiki",
                "wikidata",
                "wikivoyage",
                "wikidata"
            ],
            "position": [
                "whole"
            ]
        }
    ]
}
